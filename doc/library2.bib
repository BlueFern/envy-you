journal = {Conference proceedings : ... Annual International Conference of the IEEE Engineering in Medicine and Biology Society. IEEE Engineering in Medicine and Biology Society. Conference},
keywords = {Animals,Aorta,Aorta: physiology,Blood Pressure,Blood Pressure: physiology,Computer Simulation,Dogs,Femoral Artery,Femoral Artery: physiology,Models, Cardiovascular,Pulsatile Flow,Pulsatile Flow: physiology},
month = jan,
pages = {1385--8},
pmid = {19162926},
title = {{Estimation of the aortic pressure waveform from a peripheral artery pressure waveform via an adaptive transfer function.}},
