file = {:Users/Kathi/Library/Application Support/Mendeley Desktop/Downloaded/Zetterberg et al. - 2011 - Hypoxia due to cardiac arrest induces a time-dependent increase in serum amyloid $\beta$ levels in humans.pdf:pdf},
issn = {1932-6203},
