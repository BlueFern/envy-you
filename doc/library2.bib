keywords = {Adult,Aged,Aged, 80 and over,Amyloid beta-Peptides,Amyloid beta-Peptides: blood,Anoxia,Anoxia: blood,Anoxia: etiology,Biological Assay,Brain,Brain: metabolism,Brain: pathology,Female,Heart Arrest,Heart Arrest: blood,Heart Arrest: complications,Humans,Intensive Care Units,Male,Middle Aged,Patient Admission,Peptide Fragments,Peptide Fragments: blood,Resuscitation,Time Factors},
month = jan,
number = {12},
