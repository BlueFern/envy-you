issn = {1553-7358},
journal = {PLoS computational biology},
keywords = {Animals,Astrocytes,Astrocytes: metabolism,Bicarbonates,Bicarbonates: metabolism,Biological,Chlorides,Chlorides: metabolism,Extracellular Space,Extracellular Space: chemistry,Extracellular Space: metabolism,Humans,Ion Transport,Ion Transport: physiology,Membrane Potentials,Membrane Potentials: physiology,Models,Neurons,Neurons: metabolism,Osmosis,Osmosis: physiology,Paracrine Communication,Paracrine Communication: physiology,Potassium,Potassium: metabolism,Sodium,Sodium-Potassium-Exchanging ATPase,Sodium-Potassium-Exchanging ATPase: metabolism,Sodium: metabolism,Systems Biology},
month = jan,
number = {1},
pages = {e1000272},
pmid = {19165313},
title = {{Astrocytic mechanisms explaining neural-activity-induced shrinkage of extraneuronal space.}},
volume = {5},
year = {2009}
}
@article{Koenigsberger2005,
abstract = {It is well-known that cyclic variations of the vascular diameter, a phenomenon called vasomotion, are induced by synchronous calcium oscillations of smooth muscle cells (SMCs). However, the role of the endothelium on vasomotion is unclear. Some experimental studies claim that the endothelium is necessary for synchronization and vasomotion, whereas others report rhythmic contractions in the absence of an intact endothelium. Moreover, endothelium-derived factors have been shown to abolish vasomotion by desynchronizing the calcium signals in SMCs. By modeling the calcium dynamics of a population of SMCs coupled to a population of endothelial cells, we analyze the effects of an SMC vasoconstrictor stimulation on endothelial cells and the feedback of endothelium-derived factors. Our results show that the endothelium essentially decreases the SMCs calcium level and may move the SMCs from a steady state to an oscillatory domain, and vice versa. In the oscillatory domain, a population of coupled SMCs exhibits synchronous calcium oscillations. Outside the oscillatory domain, the coupled SMCs present only irregular calcium flashings arising from noise modeling stochastic opening of channels. Our findings provide explanations for the published contradictory experimental observations.},
author = {Koenigsberger, Mich\`{e}le and Sauser, Roger and B\'{e}ny, Jean-Louis and Meister, Jean-Jacques},
doi = {10.1529/biophysj.104.054965},
file = {:Users/Kathi/Library/Application Support/Mendeley Desktop/Downloaded/Sauser, Be - 2005 - Role of the Endothelium on Arterial Vasomotion.pdf:pdf},
institution = {Ecole Polytechnique F\'{e}d\'{e}rale de Lausanne, Laboratory of Cell Biophysics, Lausanne, Switzerland. michele.koenigsberger@epfl.ch},
issn = {0006-3495},
journal = {Biophysical Journal},
keywords = {Arteries,Arteries: cytology,Arteries: physiology,Biological Factors,Biological Factors: physiology,Biophysical Phenomena,Biophysics,Calcium Signaling,Cell Communication,Endothelium, Vascular,Endothelium, Vascular: cytology,Endothelium, Vascular: physiology,Mathematics,Models, Cardiovascular,Muscle Contraction,Muscle Contraction: physiology,Muscle, Smooth, Vascular,Muscle, Smooth, Vascular: cytology,Muscle, Smooth, Vascular: physiology,Vasoconstriction,Vasoconstriction: physiology},
month = jun,
