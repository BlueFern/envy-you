issn = {1095-8541},
journal = {Journal of Theoretical Biology},
keywords = {animals,calcium,calcium activated,calcium activated physiology,calcium channels,calcium channels physiology,calcium metabolism,membrane potentials,membrane potentials physiology,mesentery,mesentery blood supply,microcirculation,microcirculation drug effects,microcirculation physiology,muscle,myocytes,nitric oxide,nitric oxide pharmacology,potassium channels,proteins,proteins pharmacology,rats,ryanodine receptor calcium release channel,ryanodine receptor calcium release channel metabo,sarcoplasmic reticulum,sarcoplasmic reticulum metabolism,smooth,smooth muscle,smooth muscle drug effects,smooth muscle metabolism,vascular,vascular cytology,vascular drug effects,vascular metabolism,voltage gated,voltage gated physiology},
