issn = {0168-0102},
journal = {Neuroscience Research},
keywords = {animals,cerebral arteries,cerebral arteries drug effects,cerebral arteries metabolism,cerebrovascular circulation,cerebrovascular circulation drug effects,cerebrovascular circulation physiology,cyclooxygenase 2 inhibitors,cyclooxygenase 2 inhibitors pharmacology,evoked potentials,flavoproteins,fluorescent dyes,group iv phospholipases a2,group iv phospholipases a2 genetics,group iv phospholipases a2 metabolism,hyperemia,hyperemia metabolism,inbred c57bl,knockout,male,mice,nitric oxide,nitric oxide metabolism,nitric oxide synthase type i,nitric oxide synthase type i antagonists \& inhibi,nitric oxide synthase type i genetics,nitric oxide synthase type i metabolism,nitroarginine,nitroarginine pharmacology,physical stimulation,somatosensory,somatosensory cortex,somatosensory cortex blood supply,somatosensory cortex drug effects,somatosensory cortex metabolism,somatosensory physiology,vasodilation,vasodilation drug effects,vasodilation physiology},
